{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "npx nodemon",
    "build": "rimraf ./dist && tsc && tsc-alias",
    "start": "node dist/index.js",
    "db:migrate": "cross-env NAME=$NAME npx prisma migrate dev --name %NAME% --create-only && npx prisma generate",
    "db:rollback": "npx prisma migrate reset --force && npx prisma generate",
    "db:push": "npx prisma db push",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run lint:fix",
      "git add ."
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
